  "scripts": {
    "service:install": "cross-env NODE_ENV=production node src/install.js",
    "service:uninstall": "cross-env NODE_ENV=production node src/uninstall.js",
    "dev": "cross-env NODE_ENV=dev node src/server.js",
    "debug": "cross-env NODE_ENV=dev nodemon --inspect --inspect-port 9030 src/server.js",
