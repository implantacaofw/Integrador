    "moment": "^2.27.0",
    "mysql2": "^2.1.0",
    "node-java": "^1.0.1",
    "node-windows": "^1.0.0-beta.5",
    "ramda": "^0.27.0",
    "sequelize": "^6.3.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
